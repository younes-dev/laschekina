                                {% if app.user != null and  member.id == app.user.id  and entity.userEmitter.id == member.id  %}
                                    {% set sizeTop =  5  %}
                                    {% if app.user.id|contactmessaging(entity.userReceiver.id ) %}
                                        <span class="badge badge-danger badge-counter notif-msg  notif-msg-liste-amis" style="margin-top:{{ sizeTop }}px !important;"  >
                                            <i class="zmdi zmdi-email   icon-notif"  ></i>   </span>
                                        {% set sizeTop =  sizeTop + 30  %}
                                    {% endif %}

                                    {% if app.user.id|filterCollectionNews(entity.userReceiver.id ) > 0  %}
                                        <span class="badge badge-danger badge-counter notif-msg  notif-msg-liste-amis display-mobile" style="margin-top:{{ sizeTop }}px !important;"  >
                                            <i class="zmdi zmdi-file-text  icon-notif "  ></i>
                                        </span>
                                        {% set sizeTop = sizeTop + 30  %}
                                    {% endif %}
                                    {% if app.user.id|CollectionPhoto(entity.userReceiver.id )> 0  %}
                                        <span class="badge badge-danger badge-counter notif-msg  notif-msg-liste-amis  display-mobile"  style="margin-top:{{ sizeTop }}px !important;"   >
                                            <i class="zmdi zmdi-image  icon-notif " ></i>
                                        </span>
                                        {% set sizeTop = sizeTop + 30  %}
                                    {% endif %}
                                    {% if app.user.id|CollectionMedia(entity.userReceiver.id )> 0  %}
                                        <span class="badge badge-danger badge-counter notif-msg  notif-msg-liste-amis  display-mobile" style="margin-top:{{ sizeTop }}px !important;"    >
                                            <i class="zmdi zmdi-tv-alt-play  icon-notif "  ></i>
                                        </span>
                                        {% set sizeTop = sizeTop + 30  %}
                                    {% endif %}
                                    {% if  app.user.id|CollectionMedia(entity.userReceiver.id ) > 0  or  app.user.id|CollectionPhoto(entity.userReceiver.id ) > 0  or  app.user.id|filterCollectionNews(entity.userReceiver.id ) > 0  %}
                                        <span  class="badge badge-danger badge-counter notif-msg  notif-msg-liste-amis display-web"       style=" margin-top: {% if app.user.id|contactmessaging(entity.userEmitter.id ) %}30 {% else %}  5  {% endif %}px !important;"     >
                                            <i class="zmdi zmdi-notifications-active  icon-notif "  ></i>
                                        </span>
                                        {% set sizeTop = sizeTop + 30  %}
                                    {% endif %}
                                {% endif %}
                                <img src="  {% if entity.userReceiver.pictureprofil != null %}{{ path_picture ~ entity.userReceiver.pictureprofil }}   {% else %} {{ asset('assets/images/avatar.png') }} {% endif %} "
