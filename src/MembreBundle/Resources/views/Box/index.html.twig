                            <div class="item">
                            {% endif %}
                            {% if concertInBox|length == counter %}
                                </div>
                            {% endif %}
                            {% set counter = counter + 1 %}
                            {% set image = image + 1 %}

                            {% endfor %}
                        </div>
                    </div>
                    {% endif %}


