{% extends 'FrontBundle::layout.html.twig' %}

{% block title %}  {{ title }}{% endblock %}



{% block imgscreen %}
    <style>
        .padding-top-0{
            padding-top:0px !important;
        }
    </style>
    <!-- gallerie -->
    <div class="owl-carousel accueil-gallerie owl-theme">
        {% for entity in gallerysBanners %}
            <a href="{{ path_picture~entity.picture }}" class="item fancybox">
                <div class="img-resize imgLiquid">
                    {% if entity.bannerpicture is defined %}
                        <img src="{{ path_picture~entity.bannerpicture }}" alt="" class="resizeme "/>
                    {% else %}
                        <img src="{{ path_picture~entity.picture }}" alt="" class="resizeme "/>
                    {% endif %}
                </div>
            </a>
        {% endfor %}
    </div>
    <!-- End gallerie -->
{% endblock %}



    {% block body %}


        <div class="content-page inner-xs  ">

        <div class="container">
            <div class="row">
                <div class="col-md-9 col-xs-12 box-page-bg">
                    {% for flashMessage in app.session.flashbag.get('boxdelete') %}
                        <br/>
                        <div class="alert alert-danger alert-block"><a href="#" data-dismiss="alert" class="close">×</a>
                            {{ flashMessage }}
                        </div>
                    {% endfor %}


                    {# Liste des vip In Box #}
                    {% if vipInBox != null %}
                        <div class=" inner-vs ">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <h3>{{ 'box_listes_vip'|trans }}<i class="zmdi zmdi-chevron-right"></i></h3>
                                <div class=" 6col-md-12 col-sm-12 col-xs-12  vip-block-content mag-top-20">

                                    <div  class="owl-carousel owl-theme vip-caroussel " >
                                        {% for item in vipInBox %}
                                            <div  class="item"  >
                                                <a href="{{ path('collection',{'activeTab': "vip" ,'fullName' : item.vip.username , 'id': item.vip.id}) }}">

                                                    <div {% if vipInBox|length > 6 %}class="img-resize imgLiquid" {% endif %}>
                                                        {% if   app.user.id|contactmessaging(item.vip.id ) %}
                                                            <span class="badge badge-danger badge-counter notif-msg" style="line-height: 15px!important  ; " >      <i class="zmdi zmdi-email" style="font-size: 15px!important ; right: 3px !important;"></i>   </span>
                                                        {% endif %}
                                                        <img style=" height: 180px"
                                                             src="{% if item.vip.pictureprofil != null %}{{ path_picture ~ item.vip.pictureprofil }}{% else %}{{ path_no_image ~ 'assets/images/no-image.jpg' }}{% endif %}"
                                                             alt=""
                                                             class="image-resize"/>
                                                    </div>
                                                </a>
                                                {% if vipInBox|length > 6 %}
                                                    <div class="close-btn-image">
                                                        <a href="{{ path('front_end_page_box_archive_delete',{'id':item.id}) }}">
                                                            <i class="zmdi zmdi-close"></i></a>
                                                    </div>
                                                {% endif %}

                                                <a href="{{ path('collection',{'activeTab': "vip" ,'fullName' : item.vip.username , 'id': item.vip.id}) }}">
                                                <span {% if vipInBox|length <6 %}class="span-title" {% endif %} >

                                                               {% if item.vip.nom != null  %}    {{ item.vip.nom  }} {% else %} {{  item.vip.prenom }} {% endif %}
                                                    </span>
                                                </a>
                                                {% if vipInBox|length < 6 %}
                                                    <a href="{{ path('front_end_page_box_archive_delete',{'id':item.id}) }} "
                                                       style="float: right;  top: -7px; margin-top: -21px;">
                                                        <i class="zmdi zmdi-close"></i></a>
                                                {% endif %}


                                            </div>
                                        {% endfor %}

                                    </div>


                                </div>

                            </div>

                        </div>
                    {% endif %}


                    {# Liste des  Fan In Box #}
                    {% if vipFanInBox != null %}
                        <div class=" inner-vs ">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <h3>{{ 'box_listes_fans_vip'|trans }}<i class="zmdi zmdi-chevron-right"></i></h3>
                                <div class=" 6col-md-12 col-sm-12 col-xs-12  vip-block-content mag-top-20">

                                    <div  class="owl-carousel owl-theme vip-caroussel "  >
                                        {% for item in vipFanInBox %}
                                            <div  class="item" >
                                                <a href="{{ path('collection',{'activeTab': "vip" ,'fullName' : item.vip.username , 'id': item.vip.id}) }}">

                                                    <div {% if vipFanInBox|length > 6 %}class="img-resize imgLiquid" {% endif %}>
                                                        {% if   app.user.id|contactmessaging(item.vip.id ) %}
                                                            <span class="badge badge-danger badge-counter notif-msg" style="line-height: 15px!important  ; " >      <i class="zmdi zmdi-email" style="font-size: 15px!important ;   "></i>   </span>
                                                        {% endif %}
                                                        <img style=" height: 180px"
                                                             src="{% if item.vip.pictureprofil != null %}{{ path_picture ~ item.vip.pictureprofil }}{% else %}{{ path_no_image ~ 'assets/images/no-image.jpg' }}{% endif %}"
                                                             alt=""
                                                             class="image-resize"/>
                                                    </div>
                                                </a>
                                                {% if vipFanInBox|length > 6 %}
                                                    <div class="close-btn-image">
                                                        <a href="{{ path('front_end_page_box_archive_delete',{'id':item.id}) }}">
                                                            <i class="zmdi zmdi-close"></i></a>
                                                    </div>
                                                {% endif %}

                                                <a href="{{ path('collection',{'activeTab': "vip" ,'fullName' : item.vip.username , 'id': item.vip.id}) }}">
                                                <span {% if vipFanInBox|length < 6 %}class="span-title" {% endif %} >
                                                            <i class="zmdi zmdi-accounts-alt"></i>&nbsp;
                                                    {% if item.vip.nom != null  %}    {{ item.vip.nom  }} {% else %} {{  item.vip.prenom }} {% endif %}
                                                    </span>
                                                </a>
                                                {% if vipFanInBox|length < 6 %}
                                                    <a href="{{ path('front_end_page_box_archive_delete',{'id':item.id}) }} "
                                                       style="float: right;  top: -7px; margin-top: -21px;">
                                                        <i class="zmdi zmdi-close"></i></a>
                                                {% endif %}


                                            </div>
                                        {% endfor %}

                                    </div>


                                </div>

                            </div>

                        </div>
                    {% endif %}



                    {# Liste des pro #}
                    {% if proInBox != null %}
                        <div class=" inner-vs ">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <h3>{{ 'box_listes_pro'|trans }}<i class="zmdi zmdi-chevron-right"></i></h3>
                                <div class=" col-md-12 col-sm-12 col-xs-12  vip-block-content mag-top-20">

                                    <div  class="owl-carousel owl-theme vip-caroussel " >
                                        {% for item in proInBox %}
                                            <div  class="item"  >
                                                <a href="{{ path('collection',{'activeTab': "vip" ,'fullName' : item.vip.username , 'id': item.vip.id}) }}">

