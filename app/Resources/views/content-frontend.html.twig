     function AddOrRemoveBox(id, typeAction, typebox) {
         $('#box').hide();
         $('#loading').show();
         var data = {
             'id': id,
             'typeAction': typeAction,
             'typebox': typebox
         };
         var url = '{{ path('front_end_add_remove_box_ajax') }}';
         $.ajax({
             url: url, // La ressource ciblée
             type: 'POST', // Le type de la requête HTTP.
             data: data,
             dataType: "json",
             success: function (data) {
                 $('#loading').hide();
                 $('#box').html(data.boxHtml);
                 $('#box').show();


             },

             error: function (response) {
                 console.log(response);
             }
         });
     }

