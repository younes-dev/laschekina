    function validateForm() {


        $('#sendForm').hide();
        $('#loadingsendForm').show();
        $('#sourcevideoVipError').hide();
        $('#mediaVidoeError').hide();
        $('#mediaMP3Error').hide();
        $('#imageMediaMP3Error').hide();
        $('#mediaBookError').hide();
        $('#mediapathBookError').hide();
        $('#mediaBookLiverBdError').hide();
        $('#titleMediaError').hide();

        var radios = document.getElementsByName('typeVideo');

        for (var i = 0, length = radios.length; i < length; i++) {
            if (radios[i].checked) {
                // do whatever you want with the checked radio

                var typeVideo =radios[i].value ;
                // only one radio can be logically checked, don't check the rest
                break;
            }
        }


        var     titleMedia =  $('#titleMedia').val();

        if(titleMedia == ""){
            $('#titleMediaError').show();
            return false;
        }

         if (typeVideo == 1) {

         var     sourcevideoVip =  $('#sourcevideoVip').val();

             if(sourcevideoVip == ""){
                 $('#sourcevideoVipError').show();
                 return false;

              }
         }

          if (typeVideo == 2) {

             var     mediaVidoe =  $('#mediaVidoe').val();

             if ( mediaVidoe == "" ){
                 $('#mediaVidoeError').show();
                 return false;
             }
         }

          if (typeVideo == 3) {

             var     mediaMP3 =  $('#mediaMP3').val();
             if ( mediaMP3 == "" ){
                 $('#mediaMP3Error').show();
                 return false;
             }
            var     imageMediaMP3 =  $('#imageMediaMP3').val();
             if ( imageMediaMP3 == "" ){
                 $('#imageMediaMP3Error').show();
                 return false;
             }

         }
          if (typeVideo == 4) {

             var     mediaBook =  $('#mediaBook').val();
             if ( mediaBook == "" ){
                 $('#mediaBookError').show();
                 return false;
             }

      /*   var     mediapathBook =  $('#mediapathBook').val();
             if ( mediapathBook == "" ){
                 $('#mediapathBookError').show();
                 return false;
             }*/

         }

         return true;
